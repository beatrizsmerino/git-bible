{
	"name": "git-bible",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\"",
		"eslint:fix": "vue-cli-service lint \"**/*.{json,js,vue}\" --fix --ignore-path .eslintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix",
		"deploy": "node gh-pages-deploy.js"
	},
	"resolutions": {
		"ansi-html": "https://registry.npmjs.org/ansi-html-community/-/ansi-html-community-0.0.8.tgz",
		"ansi-regex": "https://registry.npmjs.org/ansi-regex/-/ansi-regex-5.0.1.tgz",
		"glob-parent": "https://registry.npmjs.org/glob-parent/-/glob-parent-5.1.2.tgz",
		"nth-check": "https://registry.npmjs.org/nth-check/-/nth-check-2.0.1.tgz",
		"set-value": "https://registry.npmjs.org/set-value/-/set-value-4.0.1.tgz"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"@fortawesome/vue-fontawesome": "^2.0.6",
		"core-js": "^3.19.1",
		"elliptic": "^6.5.4",
		"vue": "^2.6.14",
		"vue-clipboard2": "^0.3.3",
		"vue-i18n": "^8.26.7",
		"vue-router": "^3.5.3"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.8",
		"@vue/cli-plugin-babel": "^4.5.15",
		"@vue/cli-plugin-eslint": "^4.5.15",
		"@vue/cli-service": "^4.5.15",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"execa": "latest",
		"prettier": "^2.4.1",
		"sass": "^1.43.4",
		"sass-loader": "^7.3.1",
		"vue-template-compiler": "^2.6.14"
	}
}
